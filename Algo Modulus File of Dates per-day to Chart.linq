<Query Kind="Program" />

void Main()
{
	
	//var dates = System.IO.File.ReadAllLines(@"c:\dates.txt").Select(d => DateTime.Parse(d).Date);
	var dates = ExampleData().Select(d => DateTime.Parse(d).Date);
	
	var epoch = dates.Min();
	var groupingCount = 1;
	
	var baseGroup = Enumerable.Range(0,(dates.Max()-epoch).Days+groupingCount)
						.Select(e => epoch.AddDays(e) )
						.GroupBy(d => d.AddDays(-(d - epoch).Days % groupingCount))
						.OrderBy(o => o.Key);
	var dateGroup = dates.GroupBy(d => d.AddDays(-(d - epoch).Days % groupingCount))
						.OrderBy(o => o.Key);
	
	var reportResult = (from baseItm in baseGroup
	 join date in dateGroup on baseItm.Key equals date.Key into joinSet
	 from result in joinSet.DefaultIfEmpty()
	 select new { range = baseItm.Key, count= result?.Count()??0}
	);

	reportResult.Dump();
	reportResult.Chart(r => r.range.ToString("d"), v => v.count).Dump();
	reportResult.Average(r => r.count ).Dump("Count per Day");

}

